


[Unit]
Description=Manage Environment manages the vpd and co2 levels in one or more grow tents.
After=mosquitto.service

[Service]
Type=idle
ExecStart=/home/pi/GrowBuddies/.venv/bin/python  /home/pi/GrowBuddies/growbuddiesproject/growbuddies/manage_environment.py
Restart=always
[Install]
WantedBy=multi-user.target